config_version=5

[application]

config/name="2DPlatformHordeGame"
run/main_scene="res://test_level/test_level.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

FlareManager="*res://flare/flare_manager.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1024
window/size/viewport_height=512
window/size/mode=3
window/size/window_width_override=2048
window/size/window_height_override=1024
window/stretch/mode="viewport"
window/vsync/vsync_mode=2
mouse_cursor/custom_image="res://player/crosshair.png"
mouse_cursor/custom_image_hotspot=Vector2(16, 16)
mouse_cursor/tooltip_position_offset=Vector2(16, 16)

[dotnet]

project/assembly_name="2DPlatformHordeGame"

[editor_plugins]

enabled=PackedStringArray("res://addons/2d_shapes/plugin.cfg", "res://addons/AsepriteWizard/plugin.cfg")

